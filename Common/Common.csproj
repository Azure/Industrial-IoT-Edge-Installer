<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>IoTEdgeInstaller</RootNamespace>
    <SignAssembly>false</SignAssembly>
    <AssemblyOriginatorKeyFile>Common.snk</AssemblyOriginatorKeyFile>
    <DelaySign>false</DelaySign>
    <HighEntropyVA>true</HighEntropyVA>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="iiotedgedeploymentmanifestlinux.json" />
    <EmbeddedResource Include="iiotedgedeploymentmanifestwindows.json" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
  </ItemGroup>

</Project>

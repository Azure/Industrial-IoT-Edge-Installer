<Window x:Class="IoTEdgeInstaller.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Width="800" Height="600" WindowStartupLocation="CenterScreen">
    <Window.Background>
        <SolidColorBrush Color="#121416"/>
    </Window.Background>
    <DockPanel>
        <Grid>
            <DockPanel Margin="0">
                <Frame Margin="20,20,20,20" x:Name="_NavigationFrame" HorizontalAlignment="Stretch" NavigationUIVisibility="Hidden" FocusVisualStyle="{x:Null}" />
            </DockPanel>
        </Grid>
    </DockPanel>
</Window>
